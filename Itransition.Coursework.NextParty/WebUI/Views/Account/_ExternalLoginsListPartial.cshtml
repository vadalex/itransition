@model ICollection<AuthenticationClientData>

@if (Model.Count != 0)
{
    using (Html.BeginForm("ExternalLogin", "Account", new { ReturnUrl = ViewBag.ReturnUrl }))
    {
    @Html.AntiForgeryToken()
    <div id="socialLoginList" style="margin-top: 30px">
        <p><h4>Log in using another service</h4></p>
        <div class="btn-group">
        @foreach (AuthenticationClientData p in Model)
        {
            <button class="btn btn-lg btn-info" type="submit" name="provider" value="@p.AuthenticationClient.ProviderName" title="Log in using your @p.DisplayName account">@p.DisplayName</button>
        }
        </div>
    </div>
    }
}
